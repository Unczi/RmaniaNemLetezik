{
    "manifest_version": 2,
    "name": "Románia nem létezik",
    "description": "Minden Románia szót kicserél R*mániára",
    "version": "0.1.0",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "16.png",
            "48": "48.png",
            "128": "128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}